#include <iostream>

using namespace std;
	
void Gyro() {
    x = GetAngle()
    switch(phasedetector){
    	case 0:
    		if (area == 2){
    			return 0
			}
  		if (area != 2){
    			if (x>0) {
				myRobot.tankDrive(leftBack)
	    		}
			if (x<0) {
	   			myRobot.tankDrive(rightBack)
	   		}
			}
	    case 1:
     		return 0
//This is the draft for the gyro code.        
        
    
